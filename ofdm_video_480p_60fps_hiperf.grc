- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nimport cv2\nfrom gnuradio import gr\nimport\
      \ threading\nimport queue\nimport time\n\nclass LiveVideoTransmitter(gr.sync_block):\n\
      \    \"\"\"\n    Ultra high-performance webcam video transmitter for 480p @\
      \ 60fps\n    Optimized for maximum throughput with minimal overhead\n    \"\"\
      \"\n    def __init__(self, camera_index=0, frame_width=640, frame_height=480,\
      \ fps_target=60, frame_skip=1):\n        gr.sync_block.__init__(\n        \
      \    self,\n            name=\"LiveVideoTransmitter_HiPerf\",\n            in_sig=None,\n\
      \            out_sig=[np.uint8]\n        )\n        \n        # Parameters\n\
      \        self.camera_index = int(camera_index)\n        self.frame_width = int(frame_width)\n\
      \        self.frame_height = int(frame_height)\n        self.fps_target = float(fps_target)\n\
      \        self.frame_skip = max(1, int(frame_skip))  # Skip frames to reduce\
      \ data\n        \n        # Thread-safe frame queue with larger buffer\n   \
      \     self.frame_queue = queue.Queue(maxsize=30)\n        self.capture_thread\
      \ = None\n        self.running = False\n        self.current_frame = None\n\
      \        self.frame_pos = 0\n        self.frames_captured = 0\n        self.frames_sent\
      \ = 0\n        \n    def _capture_loop(self):\n        \"\"\"Background thread\
      \ for camera capture with frame skipping\"\"\"\n        cap = cv2.VideoCapture(self.camera_index)\n\
      \        if not cap.isOpened():\n            print(f\"ERROR: Cannot open camera\
      \ {self.camera_index}\")\n            return\n            \n        # Configure\
      \ camera for maximum performance\n        cap.set(cv2.CAP_PROP_FRAME_WIDTH,\
      \ self.frame_width)\n        cap.set(cv2.CAP_PROP_FRAME_HEIGHT, self.frame_height)\n\
      \        cap.set(cv2.CAP_PROP_FPS, self.fps_target)\n        cap.set(cv2.CAP_PROP_BUFFERSIZE,\
      \ 1)  # Minimize latency\n        \n        # Use MJPEG for higher FPS support\n\
      \        cap.set(cv2.CAP_PROP_FOURCC, cv2.VideoWriter_fourcc('M','J','P','G'))\n\
      \        \n        # Performance settings\n        cap.set(cv2.CAP_PROP_AUTOFOCUS,\
      \ 0)  # Disable autofocus for speed\n        cap.set(cv2.CAP_PROP_AUTO_EXPOSURE,\
      \ 1)  # Use auto exposure\n        \n        frame_interval = 1.0 / self.fps_target\n\
      \        next_frame_time = time.time()\n        frame_counter = 0\n        \n\
      \        print(f\"Camera opened: {self.frame_width}x{self.frame_height} @\
      \ {self.fps_target}fps, skip={self.frame_skip}\")\n        \n        while self.running:\n\
      \            ret, frame = cap.read()\n            if not ret or frame is None:\n\
      \                time.sleep(0.001)\n                continue\n            \n\
      \            self.frames_captured += 1\n            frame_counter += 1\n   \
      \         \n            # Skip frames if configured\n            if self.frame_skip\
      \ > 1 and frame_counter % self.frame_skip != 0:\n                continue\n\
      \            \n            # Convert to grayscale (fast)\n            if frame.ndim\
      \ == 3:\n                gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n  \
      \          else:\n                gray = frame\n            \n            # Resize\
      \ if needed (fast linear interpolation)\n            if gray.shape[1] != self.frame_width\
      \ or gray.shape[0] != self.frame_height:\n                gray = cv2.resize(gray,\
      \ (self.frame_width, self.frame_height), \n                              \
      \        interpolation=cv2.INTER_LINEAR)\n            \n            # Convert\
      \ to contiguous byte array\n            frame_bytes = np.ascontiguousarray(gray.ravel().astype(np.uint8))\n\
      \            \n            # Add to queue (non-blocking, drop if full)\n   \
      \         try:\n                self.frame_queue.put(frame_bytes, block=False)\n\
      \                self.frames_sent += 1\n            except queue.Full:\n   \
      \             # Queue full, skip this frame\n                pass\n         \
      \   \n            # Frame rate control\n            next_frame_time += frame_interval\
      \ / self.frame_skip\n            sleep_time = next_frame_time - time.time()\n\
      \            if sleep_time > 0:\n                time.sleep(sleep_time)\n  \
      \          else:\n                # Running behind, reset timing\n         \
      \       next_frame_time = time.time()\n        \n        cap.release()\n   \
      \     print(f\"Camera closed. Captured: {self.frames_captured}, Sent: {self.frames_sent}\"\
      )\n        \n    def start(self):\n        \"\"\"Start camera capture thread\"\
      \"\"\n        self.running = True\n        self.frames_captured = 0\n      \
      \  self.frames_sent = 0\n        self.capture_thread = threading.Thread(target=self._capture_loop,\
      \ daemon=True)\n        self.capture_thread.start()\n        return super().start()\n\
      \        \n    def work(self, input_items, output_items):\n        \"\"\"Produce\
      \ output samples - optimized for speed\"\"\"\n        out = output_items[0]\n\
      \        n = len(out)\n        \n        if n == 0:\n            return 0\n\
      \            \n        # Try to get new frames from queue (drain quickly)\n \
      \       latest_frame = None\n        consumed = 0\n        while consumed <\
      \ 5 and not self.frame_queue.empty():  # Max 5 frames per work call\n      \
      \      try:\n                latest_frame = self.frame_queue.get_nowait()\n\
      \                consumed += 1\n            except queue.Empty:\n          \
      \      break\n        \n        # Use latest frame if we got one\n        if\
      \ latest_frame is not None:\n            self.current_frame = latest_frame\n\
      \            self.frame_pos = 0\n        \n        # If no frame available, output\
      \ zeros\n        if self.current_frame is None:\n            out[:] = 0\n  \
      \          return n\n            \n        frame_len = len(self.current_frame)\n\
      \        pos = 0\n        \n        # Fast fill: copy data in chunks\n     \
      \   while pos < n:\n            remaining_in_frame = frame_len - self.frame_pos\n\
      \            remaining_in_output = n - pos\n            \n            if remaining_in_frame\
      \ <= 0:\n                # Frame exhausted, repeat from beginning\n        \
      \        self.frame_pos = 0\n                remaining_in_frame = frame_len\n\
      \            \n            # Copy chunk (vectorized)\n            chunk_size\
      \ = min(remaining_in_frame, remaining_in_output)\n            out[pos:pos+chunk_size]\
      \ = self.current_frame[self.frame_pos:self.frame_pos+chunk_size]\n        \
      \    pos += chunk_size\n            self.frame_pos += chunk_size\n        \n\
      \        return n\n        \n    def stop(self):\n        \"\"\"Stop camera\
      \ capture\"\"\"\n        self.running = False\n        if self.capture_thread\
      \ is not None:\n            self.capture_thread.join(timeout=2.0)\n        return\
      \ super().stop()\n"
    affinity: ''
    alias: ''
    camera_index: '0'
    comment: ''
    fps_target: '60'
    frame_height: '480'
    frame_skip: '1'
    frame_width: '640'
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('LiveVideoTransmitter_HiPerf', 'LiveVideoTransmitter', [('camera_index',
